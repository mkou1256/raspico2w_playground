{
    "version": 4,
    "configurations": [
        {
            "name": "WSL",
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "intelliSenseMode": "gcc-x64",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/pico-sdk/**"
            ],
            "defines": [
                "PICO_BOARD=pico2_w"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/**",
                    "${workspaceFolder}/pico-sdk/**"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ]
}
